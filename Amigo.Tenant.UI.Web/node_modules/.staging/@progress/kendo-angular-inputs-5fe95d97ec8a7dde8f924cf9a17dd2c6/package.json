{
  "name": "@progress/kendo-angular-inputs",
  "description": "Inputs Package for Angular",
  "version": "1.5.0",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "typings": "dist/es/main.d.ts",
  "jsnext:main": "dist/es/main.js",
  "author": "Progress",
  "@progress": {
    "friendlyName": "Inputs"
  },
  "license": "SEE LICENSE in LICENSE.md",
  "scripts": {
    "test": "gulp test",
    "e2e": "gulp e2e",
    "start": "gulp start",
    "lint": "gulp lint",
    "api-check": "gulp api-check",
    "build-package": "gulp build-package",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI",
    "Progress",
    "Angular",
    "Slider",
    "Switch",
    "MaskedTextBox"
  ],
  "peerDependencies": {
    "@angular/common": "^4.0.0 || ^5.0.0",
    "@angular/core": "^4.0.0 || ^5.0.0",
    "@angular/forms": "^4.0.0 || ^5.0.0",
    "@progress/kendo-angular-intl": "^1.0.0",
    "@progress/kendo-angular-l10n": "^1.0.0",
    "rxjs": "^5 <5.5"
  },
  "dependencies": {
    "@progress/kendo-angular-resize-sensor": "^2.0.0",
    "@telerik/kendo-draggable": "^1.5.1",
    "@telerik/kendo-inputs-common": "^2.2.2",
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@krux/condition-jenkins": "1.0.1",
    "@progress/kendo-angular-buttons": "^2.0.0",
    "@progress/kendo-angular-dateinputs": "^1.4.1",
    "@progress/kendo-angular-dropdowns": "^1.3.0",
    "@progress/kendo-angular-intl": "^1.0.0",
    "@progress/kendo-angular-l10n": "^1.0.0",
    "@progress/kendo-angular-tasks": "^9.0.3",
    "@progress/kendo-theme-default": "^2.0.0",
    "@telerik/semantic-prerelease": "^1.0.0",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.2.34",
    "@types/zone.js": "0.0.27",
    "cldr-data": "^32.0.0",
    "codelyzer": "^3.0.0",
    "core-js": "^2.2.2",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "^3.9.0",
    "json-loader": "^0.5.4",
    "rxjs": "^5 <5.5",
    "semantic-release": "^6.3.6",
    "tslint": "5.4.3",
    "typescript": "~2.3.0",
    "validate-commit-msg": "^1.1.1",
    "zone.js": "0.8.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-angular-inputs.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  }
}